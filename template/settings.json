{
  "spellright.language": "en",
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "vetur.format.defaultFormatter.html": "js-beautify-html",
  "vetur.format.defaultFormatterOptions": {
    "js-beautify-html": {
      "indent_size": 2,
      "html": {
        "end_with_newline": true,
        "js": {
          "indent_size": 2
        },
        "css": {
          "indent_size": 2
        }
      }
    },
    "prettier": {
      "tabWidth": 2
    }
  }
}
